{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "func-names": "off",
    "no-console": 0,
    "no-plusplus": "off",
    "no-restricted-syntax": "off",
    "wrap-iife": "off"
  }
}
